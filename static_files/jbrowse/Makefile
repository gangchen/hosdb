EXE_FILES = bin/ucsc-to-json.pl bin/remove-track.pl bin/generate-names.pl bin/prepare-refseqs.pl bin/wig-to-json.pl bin/biodb-to-json.pl bin/draw-basepair-track.pl bin/flatfile-to-json.pl bin/bam-to-json.pl

