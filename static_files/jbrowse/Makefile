	$(NOECHO) $(NOOP)

realclean ::
	$(RM_F) \
	  $(INST_SCRIPT)/prepare-refseqs.pl $(INST_SCRIPT)/wig-to-json.pl \
	  $(INST_SCRIPT)/draw-basepair-track.pl $(INST_SCRIPT)/flatfile-to-json.pl \
